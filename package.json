{
  "name": "training",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000/",
  "dependencies": {
    "@date-io/core": "1.3.6",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.44",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.9.0",
    "axios": "^0.19.2",
    "clsx": "^1.1.0",
    "connected-react-router": "6.8.0",
    "date-fns": "^2.10.0",
    "formik": "^2.1.4",
    "history": "^4.10.1",
    "immer": "^8.0.1",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "^3.4.3",
    "recharts": "^2.0.0-beta.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.4",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "concurrently": "5.3.0",
    "cypress": "5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "1.15.3",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "express": "^4.17.1",
    "husky": "4.2.5",
    "jest-enzyme": "^7.1.2",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "redux-mock-store": "^1.5.4",
    "sinon": "9.0.3",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:mock": "nodemon src/server/index.js",
    "start:both": "concurrently \"yarn start:mock\" \"yarn start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:once": "yarn run test --watchAll=false",
    "cypress:open": "yarn run cypress open",
    "cypress:run": "yarn run cypress run",
    "test:coverage": "yarn run test:once -- --coverage",
    "lint": "eslint src",
    "verify": "yarn run lint && yarn run test:coverage",
    "clean": "rm -rf node_modules && yarn install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/serviceWorker.js",
      "!src/theme/**/*.{js,jsx,ts,tsx}",
      "!src/server/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 30,
        "branches": 30,
        "functions": 30,
        "lines": 30
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
